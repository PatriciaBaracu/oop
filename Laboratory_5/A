package com.laboratory5;

public class A {
    public String a ;
    protected X x=new X ("xxx");
    protected  A(String a){
        System.out.println("constructor A cu parametru");
        this.a=a;
    }

    public A(){


        System.out.println("empty constructor de la A");
    }





   @Override
   public String toString() {
        return "A{" +
                "a='" + a + '\'' +
                ", X="+x+
              '}';
   }


}
